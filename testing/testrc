set nocompatible
filetype off
call plug#begin('~/.vim/bundle')
Plug 'andreyorst/SimpleSnippets.vim'
Plug 'andreyorst/SimpleSnippets-snippets'
call plug#end()
filetype plugin indent on
set noshowmode
let g:SimpleSnippetsExpandOrJumpTrigger = "<Tab>"
let g:SimpleSnippetsJumpBackwardTrigger = "<C-k>"
let g:SimpleSnippetsJumpToLastTrigger = "<C-j>"
let g:SimpleSnippets_search_path = $HOME."/.vim/bundle/SimpleSnippets.vim/testing/snippets/"
inoremap Qw <Esc>:x<Cr>
inoremap Qq <Esc>

